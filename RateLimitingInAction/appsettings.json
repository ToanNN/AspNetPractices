{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimitSettings": {
    "PermitLimit": 6,
    "Window": "00:00:02",
    "ReplenishmentPeriod": "00:00:01",
    "QueueLimit": 2,
    "SegmentsPerWindow": 4,
    "TokenLimit": 8,
    // for bucket token limiter
    "BucketTokenLimit": 12,
    "TokensPerPeriod": 4,
    "AutoReplenishment": true
  }
}
